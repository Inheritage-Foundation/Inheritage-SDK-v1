{
  "name": "@inheritage-foundation/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for the Inheritage API â€” India's open cultural heritage dataset.",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "clean": "rimraf dist",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "inheritage",
    "heritage",
    "open-data",
    "digital-heritage",
    "cultural-preservation",
    "api",
    "sdk",
    "typescript",
    "geojson",
    "ai",
    "openapi",
    "cc-by"
  ],
  "author": "Inheritage Foundation <api@inheritage.foundation>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Inheritage-Foundation/Inheritage-SDK-v1.git"
  },
  "bugs": {
    "url": "https://github.com/0xMilord/Inheritage-SDK-v1/issues"
  },
  "homepage": "https://inheritage.foundation/docs/api",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^20.11.20",
    "rimraf": "^5.0.5",
    "tsup": "^7.2.0",
    "typescript": "^5.4.0",
    "vitest": "^1.3.0"
  },
  "funding": "https://inheritage.foundation/donate"
}
